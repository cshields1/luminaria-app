{
  "format": "custom",
  "id": "event",
  "label": "Event",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID",
          "placeholder": ""
        },
        "type": "UID"
      },
      "type": {
        "type": "Select",
        "config": {
          "label": "Type",
          "placeholder": "",
          "options": ["Concert", "Audition"],
          "default_value": "Concert"
        }
      },
      "name": {
        "type": "Text",
        "config": {
          "label": "Name",
          "placeholder": ""
        }
      },
      "shortDescription": {
        "type": "Text",
        "config": {
          "label": "Short Description",
          "placeholder": ""
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Long Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "dateAndTime": {
        "type": "Timestamp",
        "config": {
          "label": "Date and Time",
          "placeholder": ""
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {},
          "thumbnails": []
        }
      }
    }
  }
}
